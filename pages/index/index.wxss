    /*icon默认margin-right: 13px  */
}

#location {
  margin-right: 13px;
}

#switchCity {
  font-size: 12px;
  color: #888888;
  margin-left: 7px;
}

#xinzhiWthImg {
  width: 50px;
  height: 50px;
}

.page__bd {
    padding-left: 40px;
    padding-right: 40px;
    text-align: left;
}
#showSettingPage {
  margin-top: 50%;
}
#showSettingPageText {
    margin-top: 5px;
    color: #888888;
    font-size: 18px;
    text-align: center;
}
#settingButton {
  width: 70%;
}
.icon-box{
    margin-bottom: 25px;
    display: flex;
    align-items: center;
}
.icon-box__ctn{
    flex-shrink: 100;
}
.icon-box__title{
    font-size: 20px;
}
.icon-box__desc{
    margin-top: 6px;
    font-size: 12px;
    color: #888888;
}
.icon_sp_area {
    margin-top: 10px;
    text-align: left;
}
.image{
    margin-right: 13px;
}
